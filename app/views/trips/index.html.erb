<table class="table table-striped">

  <% @trips.each do |trip| %>
    <tr>
      <td><%= trip.date.strftime("%b %d, %Y") %></td>
      <td><%= trip.group.name %></td>
      <td><%= trip.location_id %></td>
      <td><%= link_to "View", trip_path(trip) %></td>
      <td><%= link_to "Edit", edit_trip_path(trip) %></td>
    </tr>
  <% end %>

</table>